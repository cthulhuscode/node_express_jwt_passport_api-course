{
  "name": "playground",
  "version": "1.0.0",
  "main": "index.ts",
  "description": "API for a store",
  "license": "MIT",
  "scripts": {
    "tsc": "tsc",
    "build": "rimraf dist && npx tsc",
    "dev": "nodemon app.ts",
    "start": "node ./dist/app.js --env=production",
    "prestart": "npm run build",
    "lint": "eslint . --ext .ts",
    "migrations:generate": "sequelize-cli migration:generate --name",
    "migrations:run": "sequelize-cli db:migrate",
    "migrations:revert": "sequelize-cli db:migrate:undo",
    "migrations:clear": "sequelize-cli db:migrate:undo:all"
  },
  "author": "Enrique Ruvalcaba",
  "devDependencies": {
    "@types/bcrypt": "^5.0.0",
    "@types/boom": "^7.3.2",
    "@types/cors": "^2.8.13",
    "@types/express": "^4.17.17",
    "@types/jsonwebtoken": "^9.0.2",
    "@types/node": "^18.16.3",
    "@types/nodemailer": "^6.4.7",
    "@types/passport": "^1.0.12",
    "@types/passport-jwt": "^3.0.8",
    "@types/passport-local": "^1.0.35",
    "@types/pg": "^8.6.6",
    "@types/validator": "^13.7.15",
    "@typescript-eslint/eslint-plugin": "^5.59.2",
    "@typescript-eslint/parser": "^5.59.2",
    "eslint": "^8.39.0",
    "nodemon": "^2.0.22",
    "prettier": "^2.8.8",
    "sequelize-cli": "^6.6.0",
    "ts-node": "^10.9.1",
    "typescript": "^4.9.5"
  },
  "dependencies": {
    "@hapi/boom": "^10.0.1",
    "bcrypt": "^5.1.0",
    "cors": "^2.8.5",
    "dotenv": "^16.0.3",
    "express": "^4.18.2",
    "express-async-errors": "^3.1.1",
    "joi": "^17.9.2",
    "jsonwebtoken": "^9.0.0",
    "mysql2": "^3.2.4",
    "nodemailer": "^6.9.1",
    "passport": "^0.6.0",
    "passport-jwt": "^4.0.1",
    "passport-local": "^1.0.0",
    "pg": "^8.10.0",
    "reflect-metadata": "^0.1.13",
    "rimraf": "^3.0.2",
    "sequelize": "^6.31.1",
    "sequelize-typescript": "^2.1.5"
  }
}
